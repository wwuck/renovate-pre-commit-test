{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "autodiscover": true,
    "configMigration": true,
    "extends": [
        ":dependencyDashboard",
        ":ignoreModulesAndTests",
        "replacements:all",
        "workarounds:all",
        "docker:enableMajor",
        "docker:pinDigests",
        "regexManagers:dockerfileVersions",
        ":automergeDigest",
        ":automergeMinor",
        ":automergeRequireAllStatusChecks",
        ":combinePatchMinorReleases",
        ":enablePreCommit",
        ":ignoreUnstable",
        ":prNotPending",
        ":rebaseStalePrs",
        ":semanticCommits",
        ":semanticCommitScope(deps)",
        "github>whitesource/merge-confidence:beta",
        ":prHourlyLimitNone"
    ],
    "lockFileMaintenance": {
        "enabled": true
    }
}
